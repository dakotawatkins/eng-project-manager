{
  "name": "eng-project-manager",
  "version": "0.1.0",
  "description": "Initial version for engineer project manager back-end",
  "scripts": {
    "build": "npm -prefix front-end run build",
    "start": "npx concurrently 'npm run start:backend' 'npm run start:frontend'",
    "start:backend": "npm --prefix ./back-end start",
    "start:frontend": "npm --prefix ./front-end start",
    "start:dev": "nodemon src/server.js",
    "test": "npm run test:frontend && npm run test:backend",
    "test:backend": "npm test --prefix ./back-end",
    "test:frontend": "npm test --prefix ./front-end",
    "knex": "knex",
    "migrate": "knex:migrate:latest",
    "reset": "npm run rollback && npm run migrate && npm run seed",
    "rollback": "knex migrate:rollback",
    "seed": "knex seed:run"
  },
  "keywords": [],
  "author": "Dakota Watkins",
  "license": "UNLICENSED",
  "dependencies": {
    "cors": "2.8.5",
    "dotenv": "16.0.3",
    "express": "4.18.2",
    "knex": "2.4.2",
    "morgan": "1.10.0",
    "pg": "8.9.0"
  },
  "devDependencies": {
    "jest": "29.4.3",
    "nodemon": "2.0.21",
    "supertest": "6.3.3"
  },
  "directories": {
    "test": "test"
  },
  "main": "knexfile.js"
}
